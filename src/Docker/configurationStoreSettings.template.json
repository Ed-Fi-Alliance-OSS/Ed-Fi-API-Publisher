{
  "configurationStore": {
    "provider": "$PROVIDER",
    "sqlServer": {
      "connectionString": "Server=$SQLSERVER_SERVER;Database=$SQLSERVER_DATABASE;Trusted_Connection=True"
    },
    "postgreSql": {
      "connectionString": "Host=$POSTGRESQL_HOST;Database=$POSTGRESQL_DATABASE",
      "encryptionPassword": ""
    },
    "awsParameterStore": {
      "Profile": "$AWS_PROFILE",
      "Region": "$AWS_REGION"
    },
    "plainText": { }
  }
}